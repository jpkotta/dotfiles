    'fvwm-menu-directory --name MenuBrowserDotFiles --dir $HOME --check-subdir --special-dirs --links --reuse --all \
    --xterm "urxvt -e" --command-file "StartEditor \"%f\"" --command-app "Exec exec \"%f\"" \
    --icon-dir mini.folder.xpm --icon-title mini.term.xpm --icon-file mini.file.xpm --icon-app mini.hex.xpm'

# defines a popup menu built from the directory given in $0
# sends non-executable files to StartEditor
# executes executable files
DestroyFunc MenuDirectoryStartEditor
AddToFunc MenuDirectoryStartEditor
+ I PipeRead \
    'fvwm-menu-directory --func-name MenuDirectoryStartEditor --dir "$0" --check-subdir --links \
    --xterm "urxvt -e" --command-file "StartEditor \"%f\"" --command-app "StartEditor \"%f\"" \
    --icon-dir mini.folder.xpm --icon-title mini.term.xpm --icon-file mini.file.xpm --icon-app mini.hex.xpm'


