(custom-set-variables
 '(bar-cursor 2)
 '(bkup-backup-directory-info (quote ((".*" "~/.xemacs/backup" ok-create full-path))) nil (backup-dir))
 '(buffers-tab-face (quote buffers-tab))
 '(buffers-tab-max-size 100)
 '(buffers-tab-switch-to-buffer-function (quote switch-to-buffer))
 '(c-basic-offset 4)
 '(c-default-style (quote ((java-mode . "java") (other . "bsd"))))
 '(color-theme-selection "Calm Forest" nil (color-theme))
 '(column-number-mode t)
 '(cperl-indent-level 4)
 '(drag-divider-event-lag 50)
 '(font-lock-mode t nil (font-lock))
 '(get-frame-for-buffer-default-instance-limit 4)
 '(gutter-buffers-tab-visible-p t)
 '(highlight-current-line-globally t nil (highlight-current-line))
 '(lazy-shot-mode t nil (lazy-shot))
 '(line-number-mode t)
 '(mail-user-agent (quote sendmail-user-agent))
 '(modeline-3d-p t)
 '(modeline-scrolling-method (quote scrollbar))
 '(paren-mode (quote paren) nil (paren))
 '(pending-delete-mode t nil (pending-del))
 '(pending-delete-modeline-string "")
 '(tabbar-mode nil t)
 '(toolbar-visible-p nil)
 '(truncate-lines t))
(custom-set-faces
 '(default ((t (:size "11pt" :family "Fixed" :stipple nil :background "gray12" :foreground "green" :inverse-video nil :box nil :strike-through nil :overline nil :underline nil :slant normal :weight normal))))
 '(highlight-current-line-face ((t (:background "black"))))
 '(hyper-apropos-hyperlink ((((class color) (background light)) (:underline t))))
 '(region ((t (:inverse-video nil))))
 '(underline ((t nil)))
 '(zmacs-region ((t (:background "blue"))) t))
