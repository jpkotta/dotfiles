## Key bindings for ncmpc (generated by ncmpc)

## Quit
key quit = 'Q',  3

## Pause
key pause = 'P',  32

## Stop
key stop = 's'

## Seek forward
key seek-forward = 'f',  261

## Seek backward
key seek-backward = 'b',  260

## Increase volume
key volume-up = 43,  61

## Decrease volume
key volume-down = 45,  95

## Select/deselect song in playlist
key select = 'a'

## Add url/file to playlist
key add = 'A'

